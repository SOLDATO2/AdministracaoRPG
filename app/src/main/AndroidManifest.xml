<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MVVM2"
        tools:targetApi="31">
        <activity
            android:name=".view.personagem.AtualizarPersonagemActivity"
            android:exported="false" />
        <activity
            android:name=".view.personagem.GerenciarPersonagensActivity"
            android:exported="false" />
        <activity
            android:name=".view.classe.AtualizarClasseActivity"
            android:exported="false" />
        <activity
            android:name=".view.classe.GerenciarClasseActivity"
            android:exported="false" />
        <activity
            android:name=".view.raca.AtualizarRacaActivity"
            android:exported="false" />
        <activity
            android:name=".view.raca.GerenciarRacaActivity"
            android:exported="false" />
        <activity
            android:name=".view.MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.MVVM2">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>